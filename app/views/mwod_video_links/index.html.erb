<h1>Listing Video Links</h1>

<table>
  <tr>
    <th>Link</th>
    <th>Mwod post</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mwod_video_links.each do |mwod_video_link| %>
  <tr>
    <td><%= mwod_video_link.link %></td>
    <td><%= mwod_video_link.mwod_post_id %></td>
    <td><%= link_to 'Show', mwod_video_link %></td>
    <td><%= link_to 'Edit', edit_mwod_video_link_path(mwod_video_link) %></td>
    <td><%= link_to 'Destroy', mwod_video_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mwod video link', new_mwod_video_link_path %>
